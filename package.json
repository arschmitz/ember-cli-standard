{
  "name": "ember-cli-standard",
  "version": "1.0.0",
  "description": "Ember-cli standard support, for checking your application matches your coding standards.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "blueprints",
    "index.js"
  ],
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha node-tests --recursive",
    "standard": "standard",
    "nodetest": "mocha node-tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arschmitz/ember-cli-standard.git"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "bugs": {
    "url": "https://github.com/arschmitz/ember-cli-standard/issues"
  },
  "homepage": "https://github.com/arschmitz/ember-cli-standard",
  "author": "Alex Schmitz",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "console-ui": "^3.0.0",
    "ember-cli": "~3.8.1",
    "ember-cli-blueprint-test-helpers": "^0.19.2",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.8.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "express": "^4.12.3",
    "loader.js": "^4.0.1",
    "mocha": "^6.0.2",
    "testdouble": "^3.11.0"
  },
  "dependencies": {
    "broccoli-standard": "^1.1.1",
    "chalk": "^2.4.2",
    "ember-cli-babel": "^7.7.3",
    "fs-extra": "^7.0.1",
    "inflection": "^1.7.1",
    "js-string-escape": "^1.0.0",
    "rsvp": "^4.8.4",
    "silent-error": "^1.0.0",
    "standard": "^12.0.1",
    "walk-sync": "^1.1.3"
  },
  "keywords": [
    "ember-addon",
    "lint",
    "eslint",
    "eslint ember",
    "eslint ember-cli",
    "standard",
    "code quality",
    "code formatting",
    "ember standard",
    "standard ember-cli",
    "ember"
  ],
  "main": "index.js",
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-standard"
  },
  "standard": {
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "afterEach",
      "visit",
      "andThen",
      "currentURL"
    ]
  }
}
